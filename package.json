{
  "name": "tokenclient",
  "version": "1.0.0",
  "description": "Discord Token monitor and controller.",
  "main": "main.js",
  "scripts": {
    "start": "electron ./main.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds ",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^11.2.1",
    "electron": "^1.7.9"
  },
  "devDependencies": {
    "electron-packager": "^9.1.0"
  }
}
